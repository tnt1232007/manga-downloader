{
  "name": "manga-downloader",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --output-path dist",
    "tsc": "tsc --target ES6 --outDir dist --sourceMap @tsfiles.txt",
    "watch": "npm run build -- --watch",
    "watchtsc": "npm run tsc -- --watch",
    "zip": "cd dist && bestzip ../dist.zip *",
    "buildprod": "ng build --prod --output-path %npm_package_name%-%npm_package_version% & tsc --target ES6 --outdir %npm_package_name%-%npm_package_version% --sourceMap @tsfiles.txt",
    "zipprod": "cd %npm_package_name%-%npm_package_version% && bestzip ../%npm_package_name%-%npm_package_version%.zip *",
    "publish": "npm run buildprod & npm run zipprod",
    "postbuild": "npm run tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@types/chrome": "0.0.77",
    "@types/node": "~8.9.4",
    "bestzip": "^2.1.2",
    "bootstrap": "^4.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
